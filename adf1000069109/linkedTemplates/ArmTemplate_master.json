{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf1000069109"
		},
		"LS_ADLS_01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_01'"
		},
		"LS_ASQL_01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL_01'"
		},
		"DS_HTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/datablist/sample-csv-files/tree/main/files/"
		},
		"LS_ADLS_01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls1000069109.dfs.core.windows.net/"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/390bd210-33e0-4b8f-b7d6-764938e92b79/resourceGroups/1000069109_RG/providers/Microsoft.Storage/storageAccounts/adls1000069109"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf1000069109_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_01_accountKey": {
						"value": "[parameters('LS_ADLS_01_accountKey')]"
					},
					"LS_ASQL_01_connectionString": {
						"value": "[parameters('LS_ASQL_01_connectionString')]"
					},
					"DS_HTTP_properties_typeProperties_url": {
						"value": "[parameters('DS_HTTP_properties_typeProperties_url')]"
					},
					"LS_ADLS_01_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_01_properties_typeProperties_url')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}